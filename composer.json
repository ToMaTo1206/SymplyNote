{
    "name": "titit/symply-note",
    "description": "Application web minimaliste pour prendre des notes.",
    "type": "project",
  "autoload": {
    "psr-4": {
      "Authentication\\": "src/Authentication/",
      "Entity\\": "src/Entity/",
      "Service\\": "src/Service/",
      "Html\\": "src/Html/",
      "Database\\": "src/Database/"
    }
  }
,
    "authors": [
        {
            "name": "Thomas Denoyelle",
            "email": "thomas.denoyelle@etudiant.univ-reims.fr"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "fix:cs": [
            "php-cs-fixer fix"
        ],
        "test:cs" : [
            "php-cs-fixer fix --dry-run"
        ],
        "test" : [
            "@test:cs"
        ],
        "start:linux": [
            "bin/run-server.sh",
            "Composer\\Config::disableProcessTimeout"
        ],
      "start:windows": [
        "Composer\\Config::disableProcessTimeout",
        "bin\\run-server.bat"
      ],
      "start": "@start:windows"
    },



    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.88"
    },
  "require": {
    "ext-pdo": "*"
  }
}
